   <children>
      <TableView fx:id="appointmentCountTable" layoutX="103.0" layoutY="146.0" prefHeight="237.0" prefWidth="459.0">
         <columns>
            <TableColumn fx:id="monthColumn" prefWidth="146.0" text="Month" />
            <TableColumn fx:id="typeColumn" prefWidth="149.0" text="Type" />
            <TableColumn fx:id="countColumn" prefWidth="163.0" text="Count" />
         </columns>
      </TableView>
      <Button fx:id="backToAppointmentsButton" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#backToAppointmentsButtonClick" text="Back to Appointments" />
      <ComboBox fx:id="monthComboBox" layoutX="251.0" layoutY="87.0" onAction="#monthComboBoxClick" prefWidth="150.0" promptText="Select Month" />
   </children>
</AnchorPane>
