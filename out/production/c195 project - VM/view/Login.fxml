<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="227.0" prefWidth="395.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.Login">
   <children>
      <Label fx:id="userNameLabel" layoutX="30.0" layoutY="39.0" text="Username">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <Label fx:id="PasswordLabel" layoutX="30.0" layoutY="87.0" text="Password">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <Label fx:id="languageLabel" layoutX="64.0" layoutY="167.0" text="Language" />
      <Label fx:id="timeZoneLabel" layoutX="64.0" layoutY="196.0" text="Time Zone" />
      <TextField fx:id="userNameField" layoutX="178.0" layoutY="39.0" />
      <TextField fx:id="passwordField" layoutX="178.0" layoutY="87.0" />
      <Button fx:id="loginButton" layoutX="178.0" layoutY="125.0" mnemonicParsing="false" onAction="#loginButtonAction" text="Login" />
      <Button fx:id="Close" layoutX="282.0" layoutY="125.0" mnemonicParsing="false" onAction="#closeButton" text="Close" />
      <Label fx:id="timeZone" layoutX="178.0" layoutY="196.0" prefHeight="17.0" prefWidth="183.0">
         <font>
            <Font size="10.0" />
         </font></Label>
      <Label fx:id="displayLanguage" layoutX="178.0" layoutY="167.0" prefHeight="17.0" prefWidth="85.0" />
   </children>
</AnchorPane>
